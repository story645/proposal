@article{FiberBundle2020,
  title = {Fiber Bundle},
  year = {2020},
  month = may,
  abstract = {In mathematics, and particularly topology, a fiber bundle (or, in British English, fibre bundle) is a space that is locally a product space, but globally may have a different topological structure. Specifically, the similarity between a space                         E                 \{\textbackslash displaystyle E\}    and a product space                         B         \texttimes{}         F                 \{\textbackslash displaystyle B\textbackslash times F\}    is defined using a continuous surjective map                        {$\pi$}         :         E         \textrightarrow{}         B                 \{\textbackslash displaystyle \textbackslash pi \textbackslash colon E\textbackslash to B\}   that in small regions of E behaves just like a projection from corresponding regions of                         B         \texttimes{}         F                 \{\textbackslash displaystyle B\textbackslash times F\}    to                         B                 \{\textbackslash displaystyle B\}   .  The map                         {$\pi$}                 \{\textbackslash displaystyle \textbackslash pi \}   , called the projection or submersion of the bundle, is regarded as part of the structure of the bundle.  The space                         E                 \{\textbackslash displaystyle E\}    is known as the total space of the fiber bundle,                         B                 \{\textbackslash displaystyle B\}    as the base space, and                         F                 \{\textbackslash displaystyle F\}    the fiber. In the trivial case,                         E                 \{\textbackslash displaystyle E\}    is just                         B         \texttimes{}         F                 \{\textbackslash displaystyle B\textbackslash times F\}   , and the map {$\pi$} is just the projection from the product space to the first factor.  This is called a trivial bundle. Examples of non-trivial fiber bundles include the M\"obius strip and Klein bottle, as well as nontrivial covering spaces.  Fiber bundles such as the tangent bundle of a manifold and more general vector bundles play an important role in differential geometry and differential topology, as do principal bundles. Mappings between total spaces of fiber bundles that "commute" with the projection maps are known as bundle maps, and the class of fiber bundles forms a category with respect to such mappings.  A bundle map from the base space itself (with the identity mapping as projection) to                         E                 \{\textbackslash displaystyle E\}    is called a section of                         E                 \{\textbackslash displaystyle E\}   .  Fiber bundles can be specialized in a number of ways, the most common of which is requiring that the transitions between the local trivial patches lie in a certain topological group, known as the structure group, acting on the fiber                         F                 \{\textbackslash displaystyle F\}   .},
  annotation = {Page Version ID: 954253415},
  copyright = {Creative Commons Attribution-ShareAlike License},
